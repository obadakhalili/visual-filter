{
  "name": "@visual-filter/vue3",
  "version": "1.0.0",
  "description": "🕵️‍♂️ An unopinionated advanced visual filtering component for Vue 3",
  "author": "Obada Khalili",
  "homepage": "https://github.com/obadakhalili/vue-visual-filter.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/obadakhalili/vue-visual-filter.git"
  },
  "license": "MIT",
  "keywords": [
    "vue",
    "vue3",
    "component",
    "lib",
    "advanced",
    "visual",
    "filtering"
  ],
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "main": "dist/component.cjs.js",
  "browser": "dist/component.cjs.js",
  "module": "dist/component.esm.js",
  "unpkg": "dist/component.min.js",
  "scripts": {
    "dev": "vite --port 8081",
    "dev:rebuild": "yarn run dev --force",
    "format": "yarn run prettier --ignore-path ../../.gitignore --write",
    "build": "rollup --config ../../rollup.config.js --environment NODE_ENV:production,package:vue3",
    "clean": "rm -rf dist",
    "prepare": "npm run build",
    "version": "npm run format . && git add .",
    "postversion": "git push origin main && git push origin main"
  },
  "dependencies": {
    "@visual-filter/applyer": "0.0.2",
    "@visual-filter/common": "1.0.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "1.2.1",
    "@vue/compiler-sfc": "3.0.11",
    "prettier": "2.3.1",
    "rollup": "2.45.1",
    "vite": "2.1.5",
    "vue": "3.0.11"
  },
  "peerDependencies": {
    "vue": "^3.0.11"
  },
  "engines": {
    "node": ">=12"
  }
}

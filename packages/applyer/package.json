{
  "name": "@visual-filter/applyer",
  "version": "0.0.2",
  "description": "A utility for applying a filter to data.",
  "author": "Obada Khalili",
  "homepage": "https://github.com/obadakhalili/vue-visual-filter.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/obadakhalili/vue-visual-filter.git"
  },
  "license": "MIT",
  "keywords": [
    "visual-filter"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "yarn build --watch",
    "format": "yarn run prettier --ignore-path ../../.gitignore --write",
    "build": "tsc --project tsconfig.build.json",
    "clean": "rm -rf dist",
    "prepare": "yarn build",
    "version": "yarn format . && git add .",
    "postversion": "git push origin main && git push origin main --tags"
  },
  "dependencies": {
    "@visual-filter/common": "1.0.1"
  },
  "devDependencies": {
    "prettier": "2.3.1",
    "typescript": "4.2.4"
  }
}
